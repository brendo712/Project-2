<div class="container">
  <%- include('../partials/header.ejs') %>
  <%- include('../partials/nav.ejs') %>
          <h1>Edit Clubs Page</h1>
          <form action="/clubs/<%=club.id%>?_method=PUT" method="POST">
      		<p>Name: <input type="text" name="name" value="<%=club.name%>"/></p>
      		<p>Wins: <input type="text" name="wins" value="<%=club.wins%>"/></p>
      	  <p>Draws: <input type="text" name="draws" value="<%=club.draws%>"/></p>
          <p>Losses: <input type="text" name="losses" value="<%=club.losses%>"/></p>
          <p>Points: <input type="text" name="points" value="<%=club.points%>"/></p>
          <p>Position: <input type="text" name="position" value="<%=club.position%>"/></p>
              <input type="submit" name="" value="Submit Changes"/>
          </form>
  <%- include('../partials/footer.ejs') %>
</div>
